    <div class="portlet box light-grey">
      <div class="portlet-title">
        <h4><i class="icon-globe"></i>{{name}}</h4>
      </div>
      <div class="portlet-body">

        <div class="clearfix"></div>

        <table class="table table-striped table-bordered table-hover" id="sample_1">
            <tbody>

                {{#each users}}
                <tr class="odd gradeX">
                  <td>{{this.first_name}} {{this.last_name}}</td>
                  <td>{{this.email}}</td>
                  <td>{{this.type}}</td>
                  <td>{{standardDate this.created_at}}</td>
                  <td>{{standardDate this.updated_at}}</td>
                  <td>
                    <a href="#" data-action="delete" data-user_id="{{this.id}}" class="btn red mini">Delete</a>
                  </td>
                </tr>
                {{/each}}

            </tbody>
        </table>


      </div>
    </div>